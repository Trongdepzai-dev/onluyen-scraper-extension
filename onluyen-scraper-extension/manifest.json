{
  "manifest_version": 3,
  "name": "OnLuyen Auto Scraper v3.7",
  "version": "3.6",
  "description": "Tool tự động lấy câu hỏi Bài tập & Bài thi từ OnLuyen.vn make by B.Trọng",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "*://*.onluyen.vn/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Chạy Scraper",
    "default_icon": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
    }
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
